{
  "name": "dndcs",
  "productName": "DnDCS",
  "version": "0.0.0",
  "description": "A Character sheet solution for Dungeons & Dragons, built using Electron, React and SQLite3",
  "main": "./src/main/index.js",
  "scripts": {
    "start": "set NODE_ENV=dev && electron .",
    "postinstall": "electron-builder install-app-deps",
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "npm run compile && electron-builder"
  },
  "build": {
    "win": {
      "target": "portable",
      "icon": "./static/icons/win/icon.ico",
      "publisherName": "Vishal Venkatraman"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheConMiz/dndcs.git"
  },
  "keywords": [
    "DnD"
  ],
  "author": "Vishal Venkatraman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheConMiz/dndcs/issues"
  },
  "homepage": "https://github.com/TheConMiz/dndcs#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "electron-webpack": "^2.6.2",
    "file-loader": "^3.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "knex": "^0.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "source-map-support": "^0.5.12",
    "sqlite3": "^4.0.8"
  }
}
