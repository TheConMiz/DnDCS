{
  "name": "dndcs",
  "productName": "DnDCS",
  "version": "0.0.0",
  "homepage": "./",
  "description": "A Character sheet solution for Dungeons & Dragons, built using Electron, React and SQLite3",
  "main": "./public/electron-main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "dev-start": "webpack && webpack-dev-server",
    "test-build": "webpack && electron-builder --dir",
    "dist": "webpack && electron-builder"
  },
  "build": {
    "appId": "com.electron.dndcs",
    "productName": "DnDCS",
    "asar": "false",
    "directories": {
      "app": "./"
    },
    "files": [
      "public/electron-main.js",
      "public/data/**/*",
      "!dist/win-unpacked/**/*",
      {
        "from": "dist/",
        "to": "public/",
        "filter": [
          "!dist/*.yaml"
        ]
      }
    ],
    "win": {
      "target": "nsis-web",
      "icon": "./public/icon.ico",
      "publisherName": "Vishal Venkatraman"
    }
  },
  "nsisWeb": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheConMiz/dndcs.git"
  },
  "keywords": [
    "DnD"
  ],
  "author": "Vishal Venkatraman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheConMiz/dndcs/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/styles": "^4.0.2",
    "antd": "^3.19.2",
    "electron-is-dev": "^1.1.0",
    "knex": "^0.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "sqlite3": "^4.0.8"
  }
}
