{
  "name": "dndcs",
  "productName": "DnDCS",
  "version": "0.0.0",
  "description": "A Character sheet solution for Dungeons & Dragons, built using Electron, React and SQLite3",
  "main": "./app/src/main/electron-main.js",
  "scripts": {
    "dev": "concurrently \"cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js\" \"cross-env NODE_ENV=development electron ./app/src/main/electron-main.js\"",
    "start": "set NODE_ENV=dev && electron .",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheConMiz/dndcs.git"
  },
  "keywords": [
    "DnD"
  ],
  "author": "Vishal Venkatraman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheConMiz/dndcs/issues"
  },
  "homepage": "https://github.com/TheConMiz/dndcs#readme",
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0",
    "file-loader": "^3.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "electron": "^5.0.2",
    "knex": "^0.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "sqlite3": "^4.0.8"
  }
}
