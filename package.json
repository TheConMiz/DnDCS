{
  "name": "dndcs",
  "productName": "DnDCS",
  "version": "0.0.0",
  "description": "A Character sheet solution for Dungeons & Dragons, built using Electron, React and SQLite3",
  "main": "./src/electron-main.js",
  "scripts": {
    "start": "electron .",
    "concurrent-start": "concurrently \"npm run test-start\" \"wait-on http://locahost:8080/ && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "test-start": "webpack-dev-server",
    "test-build": "webpack",
    "dist": "webpack && electron-builder",
    "test-dist": "electron-builder --em.main=dist/bundle.js"
  },
  "build": {
    "appId": "com.electron.dndcs",
    "productName": "DnDCS",
    "asar": false,
    "files": [
      "node_modules",
      "dist/**/*",
      "*.js"
    ],
    "win": {
      "target": "portable",
      "icon": "./static/icons/win/icon.ico",
      "publisherName": "Vishal Venkatraman"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheConMiz/dndcs.git"
  },
  "keywords": [
    "DnD"
  ],
  "author": "Vishal Venkatraman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheConMiz/dndcs/issues"
  },
  "homepage": "https://github.com/TheConMiz/dndcs#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.2",
    "electron-builder": "^20.43.0",
    "electron-is-dev": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "wait-on": "^3.2.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/styles": "^4.0.2",
    "antd": "^3.19.2",
    "knex": "^0.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "sqlite3": "^4.0.8"
  }
}
